---
layout: nil
title : RSS Feed
---

<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
    <channel>
    <title>black label creative</title>
    <description>black label creative - Jasal Vadgama</description>
    <link>http://blacklabelcreative.com/rss.xml</link>
    <link>http://blacklabelcreative.com/</link>
    <lastBuildDate>{{ site.time | date_to_xmlschema }}</lastBuildDate>
    <pubDate>{{ site.time | date_to_xmlschema }}</pubDate>
    <ttl>1800</ttl>

    {% for post in site.posts %}
        <item>
            <title>{{ post.title }}</title>
            <description>{{ post.content | xml_escape }}</description>
            <link>http://blacklabelcreative.com{{ post.url }}</link>
            <guid>http://blacklabelcreative.com{{ post.id }}</guid>
            <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
        </item>
    {% endfor %}

    </channel>
</rss>